{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/firmware/build/include",
                "${workspaceFolder}/firmware/**",
                "${workspaceFolder}/firmware/main/foundations",
                "${env:IDF_PATH}/**",
                "${env:HOME}/esp/xtensa-esp32-elf/include"
            ],
            "defines": [
                "MG_ENABLE_HTTP_STREAMING_MULTIPART",
                "LWIP_SOCKET",
                "LWIP_COMPAT_SOCKETS=2"
            ],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.14.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "${env:HOME}/esp/xtensa-esp32-elf/bin/xtensa-esp32-elf-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}